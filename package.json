{
  "name": "wp-plugin-uptrack-map",
  "private": true,
  "version": "0.0",
  "description": "Uptrack Map Wordpress Plugin",
  "type": "module",
  "scripts": {
    "docker:bash": "docker compose exec wordpress bash",
    "clean": "bun ./scripts/build.ts",
    "build": "bun ./scripts/build.ts",
    "build:watch": "bun ./scripts/build.ts --watch",
    "build:prod": "bun ./scripts/build.ts --prod",
    "tsc": "tsc",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "eslint": "eslint . --max-warnings=0",
    "eslint:fix": "bun run eslint -- --fix",
    "lint": "bun run prettier && bun run tsc && bun run eslint",
    "lint:fix": "bun run prettier:fix && bun run eslint:fix"
  },
  "dependencies": {
    "@tanstack/react-form": "^1.25.0",
    "@wordpress/api-fetch": "^7.35.0",
    "@wordpress/components": "^30.8.0",
    "@wordpress/element": "^6.35.0",
    "clsx": "^2.1.1",
    "geojson": "^0.5.0",
    "leaflet": "^1.9.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.1",
    "@eslint/js": "^9.39.1",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@types/bun": "^1.3.2",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "prettier": "^3.6.2",
    "rollup": "^4.53.2",
    "rollup-plugin-esbuild": "^6.2.1",
    "typescript-eslint": "^8.46.4"
  }
}
